extends layout

block content
  h1= title
  p Welcome to #{title}

  select(id='manufacturer')
    option AMD
    option NVidia

  button(id="addFilter") Add Filter

  button(class='subscribe') Subscribe

  button(onclick='testSend()') Test Send

  section(class = 'endpoint' style = 'display:none;width:80%')
    h3 Subscription:
    code(class = 'js-sub')
    h3 Endpoint:
    code(class = 'js-end')


  script(type="text/javascript" src='/javascripts/keys.js')
  script(type="text/javascript" src='/javascripts/main.js')
  script(type="text/javascript" src='/javascripts/sw.js')